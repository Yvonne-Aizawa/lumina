{
  "llm": {
    "base_url": "http://localhost:1234/v1",
    "api_key": "unused",
    "model": "openai/gpt-oss-20b"
  },
  "heartbeat": {
    "enabled": false,
    "interval": 20,
    "idle_threshold": 10
  },
  "mcpServers": {
    "searxng": {
      "command": "npx",
      "args": ["-y", "mcp-searxng"],
      "env": {
        "SEARXNG_URL": "http://localhost:8888"
      }
    },
    "time-server": {
      "command": "npx",
      "args": ["@mcpcentral/mcp-time"]
    }
  }
}
